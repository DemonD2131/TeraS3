{
    "name": "Switch form",
    "description": "Press the saveToolbarActivator key (default C) to swtich between your ",
    "type": "origins:active_self",
    "entity_action": {
        "type": "origins:if_else",
        "condition": {
            "type": "origins:resource",
            "resource": "tera:morsellus/form",
            "comparison": "==",
            "compare_to": 0
        },
        "if_action": {
            "type": "origins:change_resource",
            "resource": "tera:morsellus/form",
            "change": 1,
            "operation": "set"
        },
        "else_action": {
            "type": "origins:change_resource",
            "resource": "tera:morsellus/form",
            "change": 0,
            "operation": "set"
        }
    },
    "cooldown": 600,
    "hud_render": {
        "should_render": false
    },
    "key": {
        "key": "key.saveToolbarActivator",
        "continuous": false
    },
    "condition": {
        "type": "origins:constant",
        "value": true
    }
}